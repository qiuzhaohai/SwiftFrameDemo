  pod 'RxCocoa', '~> 5'
  pod 'MJRefresh'
  pod 'Alamofire'
  pod 'NSObject+Rx'
  pod 'ObjectMapper', '~> 3.4'
#  pod 'SwiftyDB'

  target 'SwiftFrameTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SwiftFrameUITests' do
    # Pods for testing
  end

end
